<p id="notice"><%= notice %></p>

<h1>Listing Surrogates Tags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @surrogates_tags.each do |surrogates_tag| %>
      <tr>
        <td><%= link_to 'Show', surrogates_tag %></td>
        <td><%= link_to 'Edit', edit_surrogates_tag_path(surrogates_tag) %></td>
        <td><%= link_to 'Destroy', surrogates_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Surrogates tag', new_surrogates_tag_path %>
