<p id="notice"><%= notice %></p>

<h1>Listing Resource Collections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resource_collections.each do |resource_collection| %>
      <tr>
        <td><%= link_to 'Show', resource_collection %></td>
        <td><%= link_to 'Edit', edit_resource_collection_path(resource_collection) %></td>
        <td><%= link_to 'Destroy', resource_collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resource collection', new_resource_collection_path %>
